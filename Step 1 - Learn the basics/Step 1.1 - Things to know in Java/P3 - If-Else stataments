Solution:-
public class Solution {
    public static String compareIfElse(int a, int b) {
        // Write your code here
        String res = "";
        if(a > b)
        {
            res = "greater";
        }
        else if(a < b)
        {
            res = "smaller";
        }
        else
        {
            res = "equal";
        }

        return res;
    }
}
