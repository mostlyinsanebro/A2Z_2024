Solution:-
public class Solution {
	public static String isPrime(int num) {
		//Your code goes here
		boolean isPrime = true;

		if(num==1)
		{
			return "NO";
		}

		for(int i=2;i*i <=num;i++)
		{
			if(num%i==0)
			{
				isPrime = false;
				break;
			}
		}

		if(isPrime)
		{
			return "YES";
		}
		else
		{
			return "NO";
		}
	}
}
